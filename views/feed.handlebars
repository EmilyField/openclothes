        .item {
    width: 100px;
    height: 100px;
    background:#ccc;
    vertical-align: top;
    display: inline-block;
    *display: inline;
    margin:10px;
    zoom: 1
}
.stretch {
    width: 100%;
    display: inline-block;
    font-size: 0;
    line-height: 0
}

#info {
    list-style: none;
}

.post {
    width: 95%;
    margin: 5px auto 5px auto;
    color: white;
    background-color:#333333;
    border: 3px white solid;
    border-radius: 10px;
    text-align: center
}

.post img {
    margin: 0 auto;
    width: 100%;
    border-radius: 10px;
    display: block;
}

